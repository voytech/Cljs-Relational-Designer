(ns templates.library-view
  (:require [ui.photo :as photo ]))
(def photo-lib (cell (list)))

(defelem library-view []
  (div
   (photo/photo-loader :class "image-loader"
                       :callback #(swap! photo-lib conj {:name (:name %),
                                                         :type (:type %),
                                                         :content (:content %)}))
   (photo/photo-list  :list photo-lib)
   ))
